{
  "name": "realtor-assist",
  "version": "1.0.0",
  "description": "A Realtor assistant app",
  "main": "server.js",
  "engines": {
    "node": "12.6.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.0.2",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.3",
    "multer": "^1.4.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "prop-types": "^15.7.2",
    "randomstring": "^1.1.5",
    "react-datepicker": "^2.11.0",
    "react-sidebar": "^3.0.2"
  }
}
